{
  "hosting": {
    "public": "dist/apps/wcn",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/index.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=60"
          }
        ]
      }
    ],
    "ignore": ["stats.json"]
  }
}
