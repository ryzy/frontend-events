{
  "name": "frontend-events-site",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "ng serve",
    "build": "ng build -prod --stats-json --environment=dev",
    "deploy": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --project dev",
    "start:prod": "ng serve -prod --environment=prod",
    "build:prod": "ng build -prod --stats-json --environment=prod",
    "deploy:prod": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --project prod",
    "lint": "ng lint --type-check",
    "test": "ng test --sourcemaps=false",
    "test:ci": "ng test --code-coverage --single-run",
    "coverage": "nyc report && nyc check-coverage",
    "e2e": "ng e2e",
    "start:poc": "ng serve --app=poc",
    "build:poc": "ng build --app=poc -prod --stats-json --environment=dev",
    "deploy:poc": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --public dist/poc --project poc",
    "analyze": "webpack-bundle-analyzer ./dist/app/stats.json",
    "serve": "node scripts/server.js"
  },
  "dependencies": {
    "@angular/animations": "5.1.2",
    "@angular/cdk": "5.0.2",
    "@angular/common": "5.1.2",
    "@angular/compiler": "5.1.2",
    "@angular/core": "5.1.2",
    "@angular/forms": "5.1.2",
    "@angular/http": "5.1.2",
    "@angular/material": "5.0.2",
    "@angular/platform-browser": "5.1.2",
    "@angular/platform-browser-dynamic": "5.1.2",
    "@angular/router": "5.1.2",
    "@angular/service-worker": "5.1.2",
    "@ngrx/effects": "4.1.1",
    "@ngrx/router-store": "4.1.1",
    "@ngrx/store": "4.1.1",
    "core-js": "2.5.1",
    "foundation-sites": "6.4.4-rc1",
    "hammerjs": "2.0.8",
    "rxjs": "5.5.2",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.2",
    "@angular/compiler-cli": "5.1.2",
    "@angular/language-service": "5.1.2",
    "@ngrx/store-devtools": "4.1.1",
    "@types/jasmine": "2.8.2",
    "@types/node": "8.0.53",
    "bundlesize": "0.15.3",
    "codelyzer": "4.0.1",
    "express-history-api-fallback": "2.2.1",
    "firebase-tools": "3.16.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "nyc": "11.4.1",
    "protractor": "5.2.0",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typescript": "2.5.3",
    "webpack-bundle-analyzer": "2.9.1"
  },
  "peerDependencies": {},
  "bundlesize": [
    {
      "maxSize": "2 kB",
      "path": "./dist/app/inline*.js"
    },
    {
      "maxSize": "250 kB",
      "path": "./dist/app/{main,common}*.js"
    },
    {
      "maxSize": "50 kB",
      "path": "./dist/app/polyfill*.js"
    },
    {
      "maxSize": "20 kB",
      "path": "./dist/app/styles*.css"
    }
  ]
}
