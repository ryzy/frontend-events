{
  "name": "what-conference-next",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "ng serve",
    "start:debug": "echo 'Debug mode (slow), with source maps etc...' && ng serve -c debug",
    "build": "ng build",
    "build:dev": "ng build -c development",
    "build:prod": "ng build -c production",
    "coverage": "nyc report && nyc check-coverage",
    "lint": "prettier --config .prettierrc.json --list-different '{cypress,scripts,src}/**/*.{js,ts,json,scss}' && echo 'prettier OK, doing ng lint...' && ng lint",
    "lint-fix": "prettier --config .prettierrc.json --write '{cypress,scripts,src}/**/*.{js,ts,json,scss,yml,yaml}'; ng lint",
    "deploy": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --project dev",
    "deploy:prod": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --project prod",
    "test": "ng test --watch --sourceMap=false",
    "test:ci": "ng test --watch=false --code-coverage",
    "serve": "node scripts/server.js",
    "e2e": "DEBUG=cypress:*,-cypress:server:timers,-cypress:server:specs,-cypress:server:events,-cypress:server:file,-cypress:server:cors,-cypress:server:proxy,-cypress:server:project cypress open",
    "e2e:ci": "cypress run --browser=electron",
    "analyze": "webpack-bundle-analyzer ./dist/app/stats.json ./dist/app/ --mode=static --report=dist/size-report/index.html --no-open",
    "stitch:login": "stitch-cli login --username=$ATLAS_MONGODB_API_USER --api-key=$ATLAS_MONGODB_API_KEY --yes",
    "prestitch:export:dev": "rm -rf stitch/dev-what-conference-next",
    "prestitch:export:prod": "rm -rf stitch/what-conference-next",
    "stitch:export:dev": "stitch-cli export --app-id=dev-what-conference-next-kzfdc --output=stitch/dev-what-conference-next --yes",
    "stitch:export:prod": "stitch-cli export --app-id=what-conference-next-eqvxa --output=stitch/what-conference-next --yes",
    "stitch:import:dev": "stitch-cli import --app-id=dev-what-conference-next-kzfdc --app-name=dev-what-conference-next --path=stitch/dev-what-conference-next --strategy=replace",
    "stitch:import:prod": "stitch-cli import --app-id=what-conference-next-eqvxa --app-name=what-conference-next --path=stitch/what-conference-next --strategy=replace",
    "all": "yarn install --frozen-lockfile && yarn lint-fix && yarn test:ci && yarn coverage && yarn build:dev && yarn analyze"
  },
  "dependencies": {
    "@angular/animations": "6.1.0",
    "@angular/cdk": "6.4.1",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/forms": "6.1.0",
    "@angular/http": "6.1.0",
    "@angular/material": "6.4.1",
    "@angular/platform-browser": "6.1.0",
    "@angular/platform-browser-dynamic": "6.1.0",
    "@angular/router": "6.1.0",
    "@angular/service-worker": "6.1.0",
    "@ngrx/effects": "6.0.1",
    "@ngrx/entity": "6.0.1",
    "@ngrx/router-store": "6.0.1",
    "@ngrx/store": "6.0.1",
    "core-js": "2.5.7",
    "foundation-sites": "6.5.0-rc.2",
    "hammerjs": "2.0.8",
    "lodash-es": "4.17.10",
    "mongodb-stitch-browser-sdk": "4.0.12",
    "rxjs": "6.2.2",
    "rxjs-compat": "6.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.1",
    "@angular/cli": "6.1.1",
    "@angular/compiler-cli": "6.1.0",
    "@angular/language-service": "6.1.0",
    "@cypress/webpack-preprocessor": "2.0.1",
    "@ngrx/store-devtools": "6.0.1",
    "@types/jasmine": "2.8.8",
    "@types/lodash-es": "4.17.1",
    "@types/node": "10.5.4",
    "codelyzer": "4.4.2",
    "cypress": "3.1.0",
    "express-history-api-fallback": "2.2.1",
    "firebase-tools": "4.0.0",
    "jasmine-core": "3.1.0",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.2.0",
    "mongodb-stitch-cli": "1.1.0",
    "ngrx-store-freeze": "0.2.4",
    "nyc": "12.0.2",
    "prettier": "1.14.0",
    "rxjs-tslint-rules": "4.7.0",
    "ts-loader": "4.4.2",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "2.9.2",
    "webpack-bundle-analyzer": "2.13.1"
  },
  "peerDependencies": {}
}
