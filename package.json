{
  "name": "what-conference-next",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "ng serve",
    "start:prod": "ng serve --prod -c production",
    "build": "ng build --stats-json",
    "build:prod": "ng build --prod --stats-json -c production",
    "coverage": "nyc report && nyc check-coverage",
    "lint":
      "prettier --config .prettierrc.json --list-different '**/*.js' '**/*.ts' '**/*.json' && echo 'prettier OK, doing ng lint...' && ng lint",
    "lint-fix": "prettier --config .prettierrc.json --write '**/*.js' '**/*.ts' '**/*.json'",
    "deploy": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --project dev",
    "deploy:prod": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --project prod",
    "test": "ng test --watch",
    "test:ci": "ng test --code-coverage",
    "e2e": "cypress open",
    "e2e:ci": "cypress run",
    "analyze": "webpack-bundle-analyzer ./dist/app/stats.json",
    "serve": "node scripts/server.js"
  },
