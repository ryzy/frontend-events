{
  "name": "what-conference-next",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "ng serve",
    "start:debug": "echo 'Debug mode (slow), with source maps etc...' && ng serve -c debug",
    "build": "ng build",
    "build:dev": "ng build -c development",
    "build:prod": "ng build -c production",
    "coverage": "nyc report && nyc check-coverage",
    "lint": "prettier --config .prettierrc.json --list-different '{cypress,scripts,src}/**/*.{js,ts,json,scss}' && echo 'prettier OK, doing ng lint...' && ng lint",
    "lint-fix": "prettier --config .prettierrc.json --write '{cypress,scripts,src}/**/*.{js,ts,json,scss}'",
    "deploy": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --project dev",
    "deploy:prod": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --project prod",
    "test": "ng test --watch --sourceMap=false",
    "test:ci": "ng test --watch=false --code-coverage",
    "serve": "node scripts/server.js",
    "e2e": "DEBUG=cypress:*,-cypress:server:timers,-cypress:server:specs,-cypress:server:events,-cypress:server:file,-cypress:server:cors,-cypress:server:proxy,-cypress:server:project cypress open",
    "e2e:ci": "DEBUG=cypress:launcher cypress run --browser=chrome",
    "analyze": "webpack-bundle-analyzer ./dist/app/stats.json ./dist/app/ --mode=static --report=dist/size-report/index.html --no-open"
  },
  "dependencies": {
    "@angular/animations": "6.0.7",
    "@angular/cdk": "6.3.2",
    "@angular/common": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/forms": "6.0.7",
    "@angular/http": "6.0.7",
    "@angular/material": "6.3.2",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/router": "6.0.7",
    "@angular/service-worker": "6.0.7",
    "@ngrx/effects": "6.0.1",
    "@ngrx/entity": "6.0.1",
    "@ngrx/router-store": "6.0.1",
    "@ngrx/store": "6.0.1",
    "angularfire2": "5.0.0-rc.11",
    "core-js": "2.5.7",
    "fireauth": "1.0.4",
    "firebase": "5.2.0",
    "foundation-sites": "6.5.0-rc.1",
    "hammerjs": "2.0.8",
    "lodash-es": "4.17.10",
    "rxjs": "6.2.1",
    "rxjs-compat": "6.2.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.0-beta.1",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@cypress/webpack-preprocessor": "2.0.1",
    "@firebase/app-types": "0.3.2",
    "@ngrx/store-devtools": "6.0.1",
    "@types/jasmine": "2.8.8",
    "@types/lodash-es": "4.17.0",
    "@types/node": "10.5.2",
    "codelyzer": "4.4.2",
    "cypress": "3.0.2",
    "express-history-api-fallback": "2.2.1",
