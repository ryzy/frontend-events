{
  "name": "frontend-events-site",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "ng serve",
    "build": "ng build -prod --stats-json --environment=dev",
    "deploy": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --project dev",
    "start:prod": "ng serve -prod --environment=prod",
    "build:prod": "ng build -prod --stats-json --environment=prod",
    "deploy:prod": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --project prod",
    "lint": "ng lint --type-check",
    "test": "ng test --sourcemaps=false",
    "test:ci": "ng test --code-coverage --single-run",
    "coverage": "nyc report && nyc check-coverage",
    "e2e": "ng e2e",
    "start:poc": "ng serve --app=poc",
    "build:poc": "ng build --app=poc -prod --stats-json --environment=dev",
    "deploy:poc": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --public dist/poc --project poc",
    "analyze": "webpack-bundle-analyzer ./dist/app/stats.json",
    "serve": "node scripts/server.js"
  },
