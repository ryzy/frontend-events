{
  "id": "5b7306104fdd1f63f4d4342d",
  "database": "wcn-db",
  "collection": "events",
  "roles": [
    {
      "name": "editor",
      "apply_when": {
        "%%true": {
          "%function": {
            "arguments": ["%%user"],
            "name": "isEditor"
          }
        }
      },
      "write": true,
      "insert": true,
      "delete": true,
      "additional_fields": {}
    },
    {
      "name": "owner",
      "apply_when": {
        "origin.authorId": "%%user.id"
      },
      "write": true,
      "insert": true,
      "delete": true,
      "additional_fields": {}
    },
    {
      "name": "default",
      "apply_when": {},
      "read": true,
      "write": false,
      "insert": false,
      "delete": false,
      "additional_fields": {}
    }
  ],
  "schema": {
    "properties": {
      "_id": {
        "bsonType": "objectId"
      }
    }
  }
}
