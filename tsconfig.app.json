{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "strict": true,
    "types": []
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "preserveWhitespaces": false,
    "strictInjectionParameters": true,
    "diagnostics": true,
    "disableTypeScriptVersionCheck": true
  },
  "exclude": [
    "src/test.ts", "**/*.spec.ts", "cypress", "testing/"
  ]
}
